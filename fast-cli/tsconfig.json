{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "cli",
    "pretty": true,
    "moduleResolution": "node"
  },
  "include": [
      "index.ts"
  ],
  "exclude": [
    "node_modules",
    "cli",
    "./interface.ts"
  ]
}